<%= render :partial => "layouts/cms_templates/page_header" %>

<%= render :partial => "layouts/cms_templates/page_top" %>

  <!-- Page Content -->
  <div class="container">

    <!-- Page Heading/Breadcrumbs -->
    <div class="row">
      <div class="col-lg-12">
        <h1 class="page-header"><%= sanitize(@current_page.title, tags: %w(br small)) %></h1>
      </div>
    </div>

    <div class="row">
      <div class="col-lg-12">
        <%= content_by_name :body %>
      </div>
    </div>

    <%= render :partial => "layouts/cms_templates/page_footer" %>

  </div>
  <!-- /.container -->
  <%= content_for :footer_scripts %>

</body>
</html>
